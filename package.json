{
  "name": "mte-helpers",
  "version": "2.1.1-beta",
  "description": "The goal of this package is to make MTE as simple and easy to use as possible.",
  "scripts": {
    "dev": "esbuild src/managed/index.ts --watch --outdir=dist/cjs --format=cjs",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:types": "tsc --declarations",
    "build": "npm run build:cjs && npm run build:esm"
  },
  "keywords": [],
  "author": "Eclypses Inc.",
  "license": "ISC",
  "peerDependencies": {
    "mte": "file:mte-None-Wasm-D-RT-FLEN-JAIL-MKE-FC-2.1.2.tgz"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/managed/index.js",
  "exports": {
    ".": "./dist/cjs/managed/index.js",
    "./core/*": "./dist/cjs/core/*.js",
    "./esm": "./dist/esm/managed/index.js",
    "./esm/core/*": "./dist/esm/core/*.js"
  },
  "devDependencies": {
    "esbuild": "^0.14.42",
    "typescript": "^4.7.3"
  },
  "types": "./dist\\cjs\\managed\\index.d.ts",
  "dependencies": {
    "mte": "^2.1.2",
    "esbuild-windows-64": "^0.14.43"
  }
}
